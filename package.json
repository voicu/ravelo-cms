{
  "name": "ravelo-cms",
  "version": "0.0.3",
  "description": "Ravelo Content Management System",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --inspect=0.0.0.0:3001 server.js",
    "test": "./node_modules/.bin/lab",
    "tdd": "./node_modules/.bin/nodemon ./node_modules/.bin/lab",
    "docs:prepare": "./node_modules/.bin/gitbook install",
    "docs:build": "./bin/build",
    "docs:serve": "npm run docs:build && ./node_modules/.bin/gitbook serve",
    "docs:clean": "./node_modules/.bin/rimraf _book"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravelo-systematic-solutions/ravelo-cms.git"
  },
  "engine": {
    "npm": ">=6.1.0",
    "node": ">=10.1.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravelo-systematic-solutions/ravelo-cms/issues"
  },
  "homepage": "https://github.com/ravelo-systematic-solutions/ravelo-cms#readme",
  "dependencies": {
    "hapi": "^17.5.1",
    "mongoose": "^5.1.3"
  },
  "devDependencies": {
    "code": "^5.2.0",
    "gitbook-cli": "^2.3.2",
    "jsdoc-to-markdown": "^4.0.1",
    "lab": "^15.4.5",
    "nodemon": "^1.17.5"
  }
}
